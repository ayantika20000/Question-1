#include<stdio.h>
#include<stdlib.h>
int main()
{
	int n,i,count_A=0,count_B=0,count_C=0,vote;
	printf("How many voters can cast their vote");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("press 1 to vote for AAA,2 for BBB,3 for CCC");
		scanf("%d",&vote);
		
	}
	if(vote==1)
	{
		count_A=count_A+1;
	}
	else if(vote==2)
	{
		count_B=count_B+1;;
	}
	else if(vote==3)
	{
		count_C=count_C+1;
	}
	if(count_A>count_B && count_A>count_C)
	{
		printf("Team AAA wins the vote");
	}
	else if(count_B>count_A && count_B>count_C)
	{
		printf("Team BBB wins the vote");
	}
	else
	{
		printf("Team CCC wins the vote");
	}
	return 0;
	
}
